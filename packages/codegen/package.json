{
  "name": "@callstack/polygen-codegen",
  "version": "0.0.1",
  "description": "The code generator for @callstack/polygen",
  "license": "MIT",
  "type": "module",
  "files": ["data", "dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./w2c": {
      "types": "./dist/w2c/index.d.ts",
      "import": "./dist/w2c/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "postinstall": "tsc -p tsconfig.json || true",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@callstack/polygen-binary-utils": "workspace:^",
    "@callstack/wasm-parser": "workspace:^",
    "execa": "^9.4.1",
    "find-up": "^7.0.0",
    "glob": "^11.0.0",
    "indent-string": "^5.0.0",
    "strip-indent": "^4.0.0"
  },
  "devDependencies": {
    "@callstack/polygen-typescript-config": "workspace:^",
    "@types/node": "^22.10.0",
    "typescript": "^5.7.2"
  }
}
